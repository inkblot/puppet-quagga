{
  "name": "inkblot/quagga",
  "version": "1.0.3",
  "author": "inkblot",
  "license": "Apache-2.0",
  "summary": "A Puppet module to deploy and manage the Quagga route server",
  "source": "git://github.com/inkblot/puppet-quagga.git",
  "project_page": "https://github.com/inkblot/puppet-quagga",
